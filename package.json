{
  "name": "play-with-react-parcel",
  "version": "1.0.0",
  "description": "React &amp; parcel testing",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subuta/play-with-react-parcel.git"
  },
  "keywords": [],
  "author": "subuta <shunta.3110@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/subuta/play-with-react-parcel/issues"
  },
  "homepage": "https://github.com/subuta/play-with-react-parcel#readme",
  "scripts": {
    "clean": "rm -rf ./dist",
    "generate-imported-component": "imported-components src src/client/imported.js",
    "dev:client": "parcel watch src/client.js -d dist/client",
    "dev:server": "NODE_PATH=./src SERVER=true node -r esm ./dev.js",
    "dev": "run-p dev:client dev:server",
    "start": "NODE_ENV=production SERVER=true node dist/server/server.js",
    "hash": "hashmark --cwd dist/client -d md5 -l 8 '**/*.{js,css}' '{name}.{hash}{ext}' --asset-map assets.json",
    "build": "npm run clean && run-p build:client build:server",
    "postbuild": "run-p hash",
    "build:client": "parcel build src/client.js -d dist/client",
    "build:server": "SERVER=true parcel build src/server.js --target=node -d dist/server"
  },
  "alias": {
    "react-dom": "@hot-loader/react-dom"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "dependencies": {
    "@hot-loader/react-dom": "^16.8.6",
    "astroturf": "^0.9.2",
    "bluebird": "^3.5.4",
    "common-tags": "^1.8.0",
    "isomorphic-unfetch": "^3.0.0",
    "koa": "^2.7.0",
    "koa-body": "^4.1.0",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.11",
    "pkg-dir": "^4.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.8.4",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-ssr-prepass": "^1.0.4",
    "utils": "^0.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime-corejs2": "^7.4.4",
    "async-exit-hook": "^2.0.1",
    "autoprefixer": "^9.5.1",
    "babel-plugin-codegen": "^3.0.0",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-macros": "^2.5.1",
    "consola": "^2.6.0",
    "esm": "^3.2.22",
    "glob": "^7.1.3",
    "hashmark": "^5.0.0",
    "loud-rejection": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "postcss": "^6.0.0",
    "postcss-import": "^12.0.1",
    "postcss-modules": "^1.4.1",
    "precss": "^4.0.0",
    "sane": "^4.1.0",
    "serve": "^11.0.0",
    "tailwindcss": "^0.7.4",
    "touch": "^3.1.0",
    "tree-kill": "^1.2.1"
  }
}
